      name="imgview"
      class=".depiction.CCAContentDepictionView"
      permission="zope.Public"
      allowed_interface="eea.depiction.browser.interfaces.IImageView"
      />

  <!-- <browser:page
      for="eea.climateadapt.aceitem.IIndicator"
