    permission="cmf.ManagePortal"
    />


  <browser:page
    name="update_health_items"
    for="*"
    template="pt/update_health_items.pt"
    class=".migrate.UpdateHealthItemsFields"
    permission="cmf.ManagePortal"
    />

</configure>
