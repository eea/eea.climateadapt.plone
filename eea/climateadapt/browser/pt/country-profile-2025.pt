    <tal:block tal:define="fieldset_legend string:Biogeophysical characteristics;
                        fieldset_data data/National_Circumstances/BiogeophysicalCharacteristics">
      <tal:macro metal:define-macro="fieldset-macro">
        <div class="panel panel-default"
          tal:define="fieldset_key fieldset_key | string:_;
                      fieldset_websites fieldset_websites | nothing;
                      fieldset_data fieldset_data | nothing;
                      id python: 'accordion_' + fieldset_legend.replace(' ', '_');
                      id python: id.replace('(', '_');
                      id python: id.replace(')', '_');
                      id python: id.replace(',', '_');
                      id python: id.replace('/', '_');
                      id python: id.replace('&', '_');
                      id python: id.replace('-', '_');
                      id python: id.replace(',', '_');
                      id python: id.replace('.', '_');
                      id python: id.replace('/', '_');
                      "
          >
          <div class="panel-heading" tal:attributes="id python: id+fieldset_key+'-heading'">
            <h4 class="panel-title">
              <a class="accordion-toggle tiny-acchead" tal:attributes="href python:'#'+id+fieldset_key+'-body'" data-toggle="collapse">
                <span tal:content="fieldset_legend" i18n:translate="">Biogeophysical characteristics</span>
              </a>
            </h4>
          </div>
          <div class="panel-collapse tiny-accbody collapse" tal:attributes="id python: id+fieldset_key+'-body'">
            <div class="panel-body" tal:condition="fieldset_websites">
             <ul>
              <li tal:repeat="item fieldset_websites">
              <span>
                <a tal:attributes="href item/Url" tal:content="item/Title"></a>
                </span>
              </li>
             </ul>
            </div>
            <div class="panel-body" tal:condition="python: not fieldset_websites">
              <div tal:content="structure python: view.convert_web_int(fieldset_data)">Data here</div>
            </div>
          </div>
        </div>
      </tal:macro>
    </tal:block>
