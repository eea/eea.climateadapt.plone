            <tr tal:condition="python: 'DescribeVulnerability' in item"><td>Assessment</td><td tal:content="item/DescribeVulnerability" /></tr>
            <tr tal:condition="python: 'RiskFutureImpacts' in item"><td>Rating for the risk of potential future impacts</td><td tal:content="item/RiskFutureImpacts" /></tr>
            <tr tal:condition="python: 'RiskVariability' in item"><td>Different rating of the risk of potential future impacts</td><td tal:content="item/RiskVariability" /></tr>
            <tr tal:condition="python: 'DescribeRisk' in item"><td>Assessment</td><td tal:content="item/DescribeRisk" /></tr>
