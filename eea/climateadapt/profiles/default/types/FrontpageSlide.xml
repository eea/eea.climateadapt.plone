<?xml version="1.0" encoding="utf-8"?>
<object name="FrontpageSlide" meta_type="Dexterity FTI"
   i18n:domain="eea.climateadapt"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Frontpage Slide</property>
 <property name="description" i18n:translate=""></property>
 <property name="icon_expr"></property>
 <property name="factory">FrontpageSlide</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++FrontpageSlide</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Image"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">tabular_view</property>
 <property name="view_methods">
  <element value="listing_view"/>
  <element value="summary_view"/>
  <element value="tabular_view"/>
  <element value="full_view"/>
  <element value="album_view"/>
  <element value="event_listing"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">plone.app.contenttypes.addFolder</property>
 <property
    name="klass">eea.climateadapt.browser.frontpage_slides.FrontpageSlide</property>
 <property name="behaviors">
  <element
     value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
  <element
     value="plone.app.dexterity.behaviors.nextprevious.INextPreviousToggle"/>
  <element
     value="plone.app.multilingual.dx.interfaces.IDexterityTranslatable"/>
  <element
     value="Products.CMFPlone.interfaces.constrains.ISelectableConstrainTypes"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="plone.app.relationfield.behavior.IRelatedItems"/>
  <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
  <element value="plone.shortname"/>
  <element value="volto.blocks"/>
  <element value="plone.constraintypes"/>
 </property>
 <property
    name="schema">eea.climateadapt.browser.frontpage_slides.IFrontpageSlide</property>
 <property name="model_source"></property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
