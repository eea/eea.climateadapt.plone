  xmlns:i18n="http://namespaces.zope.org/i18n"
  xmlns:z3c="http://namespaces.zope.org/z3c"
  i18n_domain="eea.climateadapt"
>

  <!-- <include package=".maintainance" /> -->

  <browser:page
    name="admin-translation-status"
    for="*"
    class=".admin.TranslationStatus"
    permission="cmf.ManagePortal"
  />

  <browser:page
    name="find-block"
    for="*"
    class=".admin.FindContentWithBlock"
    permission="cmf.ManagePortal"
  />

  <browser:page
    for="*"
    class=".admin.CreateTranslationStructure"
    name="cts"
    permission="cmf.ManagePortal"
  />

  <browser:page
    for="*"
    template="pt/see_folder_objects_view.pt"
    name="see_folder_objects_view"
    permission="cmf.ManagePortal"
  />

